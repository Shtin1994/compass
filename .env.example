# --- .env.example ---
# Скопируйте этот файл в .env и заполните своими значениями.
# Этот файл МОЖНО и НУЖНО добавлять в систему контроля версий (Git).

# --- Application Environment & Logging ---
ENVIRONMENT=dev
LOG_LEVEL=INFO

# --- Web Server Settings ---
BACKEND_CORS_ORIGINS="http://localhost:3000,http://localhost"

# --- PostgreSQL Configuration ---
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=insight_compass_db
POSTGRES_HOST=db
POSTGRES_PORT=5432

# --- Redis Configuration ---
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB_BROKER=0
REDIS_DB_BACKEND=1

# --- Telegram API Credentials ---
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_BOT_TOKEN=
TELEGRAM_SESSION_STRING=

# --- LLM Provider ---
LLM_PROVIDER="openai"

# --- OpenAI Settings ---
OPENAI_API_KEY=

# ... и так далее для остальных настроек.